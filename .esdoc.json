{
  "source": "./lib",
  "destination": "./docs/",
  "includes": ["\\.js$"],
  "plugins": [
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "undocumentIdentifier": { "enable": false },
        "unexportedIdentifier": { "enable": true },
        "lint": { "enable": true },
        "coverage": { "enable": true },
        "typeInference": { "enable": true },
        "brand": {
          "title": "Seed Creator",
          "description": "Inserta registros en la base de datos, utilizando Sequelize.",
          "repository": "https://github.com/waquispe/seed-creator"
        }
      }
    }
  ]
}
